@model List<PhoMO.Models.Photo>


<h1>@ViewBag.Title</h1>



<table class="table">    <tr>
    <th>Name</th>
    <td>Date</td>
    <th>Focal Length</th>
    <th>ISO</th>
    <th>Shutter Speed</th>
</tr>    
    
@foreach (var photo in Model)
    {
<tr>
    <td>@photo.Name</td>
    <td>@photo.DateTime</td>
    <td>@photo.FocalLength</td>
    <td>@photo.ISO</td>
    <td>@photo.ShutterSpeed</td>
</tr>
    }
</table>


@if (Model.Count == 0)
{
    <p>No photos yet!</p>
}

<p><a asp-controller="Photo" asp-action="Add">Add Photo</a></p>