

@model List<PhoMO.Models.PhotoDate>


@{
    ViewData["Title"] = "Date";
}


        <h1> Assortment of Dates</h1>
<h5>Dates correspond to the photos taken on each date.</h5>


  @if (Model.Count == 0)
        {
            <p>No Dates yet!</p>
        }
        <p><a asp-controller="Date" asp-action="Add">Add Date</a></p>
        <div>
            <ul>
                @foreach (var date in Model)
                {
                    <li>@date.DateTime</li>
                }
            </ul>
        </div>
